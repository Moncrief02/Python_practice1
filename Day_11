
def equal_strings(string1,string2):
    str1=sorted(string1)
    str2=sorted(string2)
    if str1==str2:
        return True
    else:
        return False
print(equal_strings('love','evol'))
    
def swap_values(arr:list):
    first=arr[0]
    last=arr[-1]
    arr[0]=last
    arr[-1]=first
    return arr
list1=[2,4,67,7]
print(swap_values(list1))


